{
  "name": "server-rendered",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "dev:server": "nodemon --exec npm start",
    "dev:ui": "npm run build:ui --watch",
    "build:ui": "npx tailwindcss -i ./src/styles/globals.css -o ./public/main.css",
    "build": "NODE_ENV=production npm run build:ui"
  },
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.3",
    "express-handlebars": "^6.0.2",
    "m3o": "^1.0.1340",
    "nodemon": "^2.0.15",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "css-loader": "^6.6.0",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
